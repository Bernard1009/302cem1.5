<nav aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item active" aria-current="page">View Product</li>
  </ol>
</nav>

<div class="view-user p-5">
  {{#each rows}}
  <div class="row mb-5">
    <div class="col text-center">
      <h3>{{this.name}} from {{this.source}}</h3>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <table class="table">
        <thead>
          <tr>
            <th scope="col">ProductID</th>
            <th scope="col">Name</th>
            <th scope="col">Source</th>
            <th scope="col">Quantity</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <th scope="row">{{this.productid}}</th>
            <td>{{this.name}}</td>
            <td>{{this.source}}</td>
            <td>{{this.quantity}}</td>
          </tr>
        </tbody>
      </table>
    </div>
    <div class="row">
      <div class="col">
        <b>The Above Product Information is the Latest.</b>
      </div>
    </div>
  </div>
  {{/each}}
</div>